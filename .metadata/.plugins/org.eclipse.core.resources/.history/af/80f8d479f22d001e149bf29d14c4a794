package other;
public class ValueClass 
{
	public static void main(String[] args) 
	{
	  int i=10;
	  Integer I=Integer.valueOf(i);
	  //to convert primitive into wrapper class object
	  System.out.println(I);
	  double d=10.10;
	  Double D=Double.valueof(d);
	  System.out.println(D);
	  
	  
	}

}
